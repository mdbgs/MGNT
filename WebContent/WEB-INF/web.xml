<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>CeamiticSEV</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<!--""""""""""""""""""""""""" declaration de servlet """""""""""""""""""""""""" -->
	<servlet>
		<servlet-name>detail</servlet-name>
		<servlet-class>ServletPackages.DetailActivite</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>detail</servlet-name>
		<url-pattern>/detail</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GrapheIA</servlet-name>
		<servlet-class>ServletPackages.GrapheIndicateurActivite</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GrapheIA</servlet-name>
		<url-pattern>/indicateurActivite</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>itex</servlet-name>
		<servlet-class>ServletPackages.ItexServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>itex</servlet-name>
		<url-pattern>/pdf</url-pattern>
	</servlet-mapping>

	<!--******* utilisateurs ********* -->
	<servlet>
		<servlet-name>Connexion</servlet-name>
		<servlet-class>ServletPackages.ConnexionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Connexion</servlet-name>
		<url-pattern>/connection</url-pattern>
	</servlet-mapping>

	<!--******* Formulaire ********* -->
	<servlet>
		<servlet-name>Etudiant_Formulaire</servlet-name>
		<servlet-class>ServletPackages.EtudiantServletForm</servlet-class>
		<init-param>
			<param-name>chemin</param-name>
			<param-value>C:\fic/</param-value>
		</init-param>
		<multipart-config>
			<location>C:\fic/</location>
			<max-file-size>10485760</max-file-size>
			<max-request-size>52428800</max-request-size>
			<file-size-threshold>1048576</file-size-threshold>
		</multipart-config>
	</servlet>
	<servlet-mapping>
		<servlet-name>Etudiant_Formulaire</servlet-name>
		<url-pattern>/studentForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Enseignant_Formulaire</servlet-name>
		<servlet-class>ServletPackages.EnseignantFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Enseignant_Formulaire</servlet-name>
		<url-pattern>/teacherForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Partenaire_Formulaire</servlet-name>
		<servlet-class>ServletPackages.PartenaireFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Partenaire_Formulaire</servlet-name>
		<url-pattern>/partnerForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Responsable_Formulaire</servlet-name>
		<servlet-class>ServletPackages.ResponsableFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Responsable_Formulaire</servlet-name>
		<url-pattern>/responsableForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Animateur_Relecteur</servlet-name>
		<servlet-class>ServletPackages.Animateur_RelecteurFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Animateur_Relecteur</servlet-name>
		<url-pattern>/animatorForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ActiviteFormulaire</servlet-name>
		<servlet-class>ServletPackages.ActiviteFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActiviteFormulaire</servlet-name>
		<url-pattern>/activityForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Formation</servlet-name>
		<servlet-class>ServletPackages.FormationFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Formation</servlet-name>
		<url-pattern>/formationForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Stage_Formulaire</servlet-name>
		<servlet-class>ServletPackages.StageFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Stage_Formulaire</servlet-name>
		<url-pattern>/stageForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Reunion_Formulaire</servlet-name>
		<servlet-class>ServletPackages.ReunionFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reunion_Formulaire</servlet-name>
		<url-pattern>/meetingForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Publication_Formulaire</servlet-name>
		<servlet-class>ServletPackages.PublicationFormulaireServlet</servlet-class>
		<init-param>
			<param-name>chemin</param-name>
			<param-value>C:\fic/</param-value>
		</init-param>
		<multipart-config>
			<location>C:\fic/</location>
			<max-file-size>10485760</max-file-size>
			<max-request-size>52428800</max-request-size>
			<file-size-threshold>1048576</file-size-threshold>
		</multipart-config>
	</servlet>
	<servlet-mapping>
		<servlet-name>Publication_Formulaire</servlet-name>
		<url-pattern>/publicationForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ProgrammeCEA_formulaire</servlet-name>
		<servlet-class>ServletPackages.ProgrammeCEAFormulaireServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProgrammeCEA_formulaire</servlet-name>
		<url-pattern>/programForm</url-pattern>
	</servlet-mapping>

	<!--******* Home page ********* -->
	<servlet>
		<servlet-name>AdminAccueil</servlet-name>
		<servlet-class>ServletPackages.AdminAccueilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminAccueil</servlet-name>
		<url-pattern>/adminHome</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>responsable_Suivi_Evaluation</servlet-name>
		<servlet-class>ServletPackages.AccueilRespSuivuEvalServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>responsable_Suivi_Evaluation</servlet-name>
		<url-pattern>/RSEHome</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Responsable_Controle</servlet-name>
		<servlet-class>ServletPackages.ResponsableControleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Responsable_Controle</servlet-name>
		<url-pattern>/RCHome</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>ServletPackages.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>

	<!--******* Autres ********* -->
	<servlet>
		<servlet-name>Activite</servlet-name>
		<servlet-class>ServletPackages.ActiviteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Activite</servlet-name>
		<url-pattern>/activity</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Indicateur</servlet-name>
		<servlet-class>ServletPackages.IndicateurServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Indicateur</servlet-name>
		<url-pattern>/indicator</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GrapheIndicateur</servlet-name>
		<servlet-class>ServletPackages.GrapheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GrapheIndicateur</servlet-name>
		<url-pattern>/graphe</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Excel</servlet-name>
		<servlet-class>ServletPackages.ImportExcelServlet</servlet-class>
		<multipart-config>
			<location>/D:/fichiers/</location> <!-- A adapter chez vous -->
			<max-file-size>10485760</max-file-size> <!-- 10 Mo -->
			<max-request-size>52428800</max-request-size> <!-- 5 x 10 Mo -->
			<file-size-threshold>1048576</file-size-threshold> <!-- 1 Mo -->
		</multipart-config>
	</servlet>
	<servlet-mapping>
		<servlet-name>Excel</servlet-name>
		<url-pattern>/importExcel</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ImageServlet</servlet-name>
		<servlet-class>ServletPackages.ImageServlet</servlet-class>
		<init-param>
			<param-name>chemin</param-name>
			<param-value>C:\fic/</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImageServlet</servlet-name>
		<url-pattern>/image</url-pattern>
	</servlet-mapping>

	<!--******* Listes ********* -->
	<servlet>
		<servlet-name>Activite_liste</servlet-name>
		<servlet-class>ServletPackages.ActiviteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Activite_liste</servlet-name>
		<url-pattern>/activityList</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Indicateur_liste</servlet-name>
		<servlet-class>ServletPackages.IndicateurServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Indicateur_liste</servlet-name>
		<url-pattern>/indicatorList</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EtudiantList</servlet-name>
		<servlet-class>ServletPackages.StudentListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EtudiantList</servlet-name>
		<url-pattern>/studentList</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ResponsableList</servlet-name>
		<servlet-class>ServletPackages.ResponsableListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResponsableList</servlet-name>
		<url-pattern>/responsableList</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ListEnseignant</servlet-name>
		<servlet-class>ServletPackages.TeacherList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListEnseignant</servlet-name>
		<url-pattern>/teacherList</url-pattern>
	</servlet-mapping>

	<!--""""""""""""" Declaration du filter """""""""""""""""" -->
	<filter>
		<filter-name>ClickCounterFilter</filter-name>
		<filter-class>FilterPackage.ClickCounterFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ClickCounterFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	
